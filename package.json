{
  "name": "streamweave",
  "version": "0.1.0",
  "private": true,
  "main": "electron-main.js",
  "scripts": {
    "space-mountain-dock": "node scripts/space-mountain-client.js",
    "dev": "concurrently \"npm:space-mountain-dock\" \"npm:dev:next\" \"npm:dev:ws\" \"npm:genkit:watch\"",
    "dev:dashboard": "cross-env DASHBOARD_MODE=true concurrently \"npm:dev:next\" \"npm:dev:ws\" \"npm:genkit:watch\"",
    "dev:next": "cross-env PORT=3100 NEXT_PUBLIC_STREAMWEAVE_PORT=3100 npx next dev -p 3100 -H 0.0.0.0",
    "dev:ws": "cross-env WS_PORT=8090 tsx server.ts",
    "genkit:dev": "genkit start --port 4000 -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start --port 4000 -- tsx --watch src/ai/dev.ts",
    "build": "npx next build",
    "build:simple": "npx next build",
    "start": "npx next start",
    "lint": "npx next lint",
    "typecheck": "tsc --noEmit",
    "health-check": "node health-check.js",
    "electron": "electron .",
    "electron:dev": "set ELECTRON_IS_DEV=1 && electron .",
    "electron:kill": "taskkill /F /IM node.exe /IM electron.exe",
    "electron:pack": "electron-builder",
    "app:start": "npm run electron:dev",
    "setup": "npm run health-check"
  },
  "dependencies": {
    "@emnapi/runtime": "^1.7.0",
    "@genkit-ai/next": "^1.22.0",
    "@google-cloud/speech": "^6.5.0",
    "@google-cloud/text-to-speech": "^6.4.0",
    "@hookform/resolvers": "^4.1.3",
    "@next-auth/firebase-adapter": "^2.0.1",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@types/pako": "^2.0.4",
    "@types/wav": "^1.0.4",
    "ansi_up": "^6.0.0",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "embla-carousel-react": "^8.6.0",
    "firebase": "^11.9.1",
    "firebase-admin": "^13.6.0",
    "form-data": "^4.0.0",
    "framer-motion": "^11.5.7",
    "googleapis": "^168.0.0",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.475.0",
    "next": "15.3.3",
    "next-auth": "^4.24.13",
    "obs-websocket-js": "^5.0.7",
    "pako": "^2.1.0",
    "pusher-js": "^8.4.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "tiktok-live-connector": "^2.1.1-beta1",
    "tmi.js": "^1.8.5",
    "wav": "^1.0.2",
    "ws": "^8.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@genkit-ai/core": "^1.22.0",
    "@genkit-ai/firebase": "^1.22.0",
    "@genkit-ai/googleai": "^1.22.0",
    "@opentelemetry/exporter-jaeger": "^1.11.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/tmi.js": "^1.8.6",
    "@types/ws": "^8.5.10",
    "cross-env": "^7.0.3",
    "electron": "32.3.3",
    "electron-builder": "^25.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.3.3",
    "genkit": "^1.22.0",
    "genkit-cli": "^1.14.1",
    "patch-package": "^8.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.11.0",
    "typescript": "5.9.3"
  },
  "build": {
    "appId": "com.streamweave.dashboard",
    "productName": "StreamWeave Dashboard",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!src/**/*",
      "!docs/**/*",
      "!.next/cache/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/StreamWeaver.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/StreamWeaver.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/StreamWeaver.png"
    }
  }
}
